# VARIABLE DECLARATION
# Everything is immutable so no keyword needed

# Literals:
age = 21;        # integer
name = 'Evgeny'; # string
isHuman = true;  # bool

# Functions:
# Parentheses are mandatory
double = (n) -> {
  n * 2;
};
logDouble = (n) -> {
  log(n);
  n * 2;
};

# list
# list = [1, 2, 3];

# function plus
# result1 = +(1, 2, 3);
# result2 = + 1 2 3;
# result3 = +(...list);

# function log
# log(result1 == result2 == result3); # true
